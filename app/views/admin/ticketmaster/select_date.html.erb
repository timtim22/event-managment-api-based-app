<div class="container-scroller">
<%= render 'shared/header' %>
<div class="container-fluid page-body-wrapper">
  <%= render 'shared/sidebar' %>
 <div class="main-panel">
   <div class="col-lg-12 grid-margin stretch-card">
    <div class="card">
     <div class="card-body">
     <%= render "shared/notice" %>
     <br>
    
    <h4 class="card-title">Import Events</h4>
    <div class="table-responsive">
   
     <form action="<%= admin_ticketmaster_import_events_path %>" method="post">
      <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token() %>">
       <div class="form-group">
          <label for="start_date">Start date </label>
          <input type="text" placeholder="2020-01-05" name="start_date" id="import_start_date" class="form-control">
       </div>
    <div class="form-group">
       <label for="end_date">End date </label>
       <input type="text" placeholder="2020-01-12" name="end_date" id="import_end_date" class="form-control">
    </div>
    <div class="form-group">
     <input type="submit" class="btn btn-primary pull-right" id="import_event_btn" value="Import">
    </div> 
    </form>
    
    </div>
</div>
</div>
</div>
<%= render 'shared/loading_modal' %>
  <%=  render 'shared/footer' %>
 </div><!--main panel-->
</div><!-- page wrapper-->
</div><!--container-->